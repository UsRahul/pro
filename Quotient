#include <stdio.h>
int main() {
    int div, divisor, q, rem;
    printf("Enter dividend: ");
    scanf("%d",&div);
    printf("Enter divisor: ");
    scanf("%d",&divisor);

    // Computes quotient
    q = div / divisor;

    // Computes remainder
    rem = div % divisor;

    printf("Quotient = %d\n", q);
    printf("Remainder = %d", rem);
    return 0;
}
